{
    "name": "svetla-estetica",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "postinstall": "husky install .husky && husky add .husky/pre-commit 'npm run pre-commit' && husky add .husky/pre-push 'npm run pre-push'",
        "pre-commit": "lint-staged",
        "pre-push": "npm run check_all && npm run test",
        "dev": "next dev -p 6969",
        "build": "npm run lint && npm run type-check && next build",
        "start": "next start -p 6969",
        "test": "jest --watchAll",
        "coverage": "jest --coverage --watchAll",
        "eslint_check": "eslint 'src/*/**/*.{js,*.js,ts,*.ts,tsx,*.tsx}'",
        "stylelint_check": "stylelint 'src/**/*.{scss,*.scss,css,*.css}'",
        "prettier_check": "prettier --check 'src/**/*.{js,*.js,ts,*.ts,tsx,*.tsx,scss,*.scss,css,*.css,json}'",
        "check_all": "npm run prettier_check && npm run eslint_check && npm run stylelint_check",
        "eslint_fix": "eslint 'src/*/**/*.{js,*.js,ts,*.ts,tsx,*.tsx}' --fix",
        "stylelint_fix": "stylelint 'src/*/**/*.{scss,*.scss,css,*.css}' --fix",
        "prettier_fix": "stylelint 'src/**/*.{js,*.js,ts,*.ts,tsx,*.tsx,scss,*.scss,css,*.css,json}' --fix",
        "fix_all": "npm run prettier_fix && npm run eslint_fix && npm run stylelint_fix"
    },
    "lint-staged": {
        "src/**/*.{js,*.js,ts,*.ts,tsx,*.tsx,json}": [
            "prettier --write",
            "eslint --fix"
        ],
        "src/**/*.{scss,*.scss,css,*.css}": [
            "stylelint --fix"
        ]
    },
    "dependencies": {
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@fortawesome/react-fontawesome": "^0.1.15",
        "next": "^11.1.2",
        "nodemailer": "^6.6.2",
        "react": "17.0.2",
        "react-dom": "17.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.14.0",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
        "@babel/plugin-proposal-optional-chaining": "^7.12.7",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-classes": "^7.13.0",
        "@babel/plugin-transform-runtime": "^7.15.0",
        "@babel/plugin-transform-typescript": "^7.12.1",
        "@babel/runtime-corejs3": "^7.14.0",
        "@testing-library/jest-dom": "^5.12.0",
        "@testing-library/react-hooks": "^7.0.0",
        "@types/enzyme": "^3.10.8",
        "@types/jest": "^26.0.23",
        "@types/node": "^15.0.2",
        "@types/nodemailer": "^6.4.2",
        "@types/react": "^17.0.21",
        "@typescript-eslint/eslint-plugin": "^4.22.1",
        "@typescript-eslint/parser": "^4.22.1",
        "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
        "babel-jest": "^27.2.0",
        "babel-plugin-dynamic-import-node": "^2.3.3",
        "babel-plugin-remove-object-properties": "^1.0.2",
        "core-js": "^3.13.0",
        "enzyme": "^3.11.0",
        "enzyme-to-json": "^3.6.2",
        "eslint": "^7.25.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^7.2.0",
        "eslint-config-react-app": "^6.0.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jest": "^24.3.6",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "husky": "^6.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^27.2.0",
        "jest-fetch-mock": "^3.0.3",
        "jest-watch-typeahead": "^0.6.3",
        "lint-staged": "^11.1.2",
        "next-compose-plugins": "^2.2.1",
        "next-images": "^1.7.0",
        "prettier": "^2.3.2",
        "sass": "^1.32.12",
        "stylelint": "^13.13.1",
        "stylelint-config-prettier": "^8.0.2",
        "stylelint-config-recommended-scss": "^4.3.0",
        "stylelint-config-standard": "^22.0.0",
        "stylelint-prettier": "^1.2.0",
        "stylelint-scss": "^3.21.0",
        "typescript": "^4.2.4",
        "webpack": "^5.36.2"
    }
}
